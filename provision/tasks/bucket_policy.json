{
    "Id": "Policy1",
    "Statement": [
      {
        "Action": [
          "s3:PutObject",
          "s3:ListBucket"
        ],
        "Effect": "Allow",
        "Resource": "arn:aws:s3:::{{ bucket_name }}/*",
        "Principal": {
          "AWS": [
            "*"
          ]
        }
      }
    ]
  }  